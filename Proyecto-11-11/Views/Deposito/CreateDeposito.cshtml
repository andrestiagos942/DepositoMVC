@using Proyecto_11_11.Models.Entities;
@model Deposito;



@* HTML *@


<h1 style="text-align:center">INGRESAR PRODUCTO NUEVO</h1>

<a asp-action="Index" asp-controller="Deposito" style="margin-bottom:10px;">Volver</a>

<form asp-action="CreateDeposito">
    <div class="form-group">
        <label asp-for="Producto">PRODUCTO</label>
        <input asp-for="Producto" type="text" class="form-control" style="margin-top:5px;" />
    </div>

    <div class="form-group">
        <label asp-for="Stock">STOCK</label>
        <input asp-for="Stock" type="text" class="form-control" style="margin-top:5px;" />
    </div>

    <button type="submit" class="btn btn-outline-warning" style="margin-top:5px;">AGREGAR</button>
</form>