@using Proyecto_11_11.Models.Entities;
@model List<Producto>;



@* HTML *@



<h1 style="text-align:center">PRODUCTOS</h1>
<div class="row">
<a class="row col" asp-action="CreateProducto" asp-controller="PRoducto" style="margin-bottom:10px;">AGREGAR PRODUCTO NUEVO</a>
</div>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>NOMBRE</th>
            <th>PRECIO UNITARIO</th>
            <th>PRECIO PrecioMayorista</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var producto in Model)
        {
            <tr>
                <td>@producto.Id</td>
                <td>@producto.Nombre</td>
                <td>@producto.PrecioUnitario</td>
                <td>@producto.PrecioMayorista</td>
            </tr>
        }
    </tbody>


</table>
